                    <div class="col-md-12">
                        <div class="title-section margin-bottom-41">
                            <h2 class="title">
                                Shop Collection
                            </h2>
                        </div>
                        <nav id="mainnav" class="mainnav">
                            <ul class="menu " style="margin: 0px auto;display: flex; justify-content: center; align-items: center;">
                                @{
                                    string querycat = "SELECT * FROM tblCategory where isDel='false' order by catName";
                                    SqlDataAdapter sdacat = new SqlDataAdapter(querycat, con);
                                    DataTable dtcat = new DataTable();

                                    sdacat.Fill(dtcat);

                                    foreach (DataRow drcat in dtcat.Rows)
                                    {
                                        <li style="margin-right: 50px;">
                                            <a href="#" data-filter=".@drcat["catId"]">
                                                @drcat["catName"]
                                            </a>
                                            <ul class="submenu" style="width:250px;">
                                                @{
                                                    string querysubcat = "SELECT * FROM tblSubCategory where isDel='false' and catId='" + drcat["catId"] + "' order by subCatName";
                                                    SqlDataAdapter sdasubcat = new SqlDataAdapter(querysubcat, con);
                                                    DataTable dtsubcat = new DataTable();

                                                    sdasubcat.Fill(dtsubcat);
                                                    foreach (DataRow drsubcat in dtsubcat.Rows)
                                                    {

                                                        <li>
                                                            <a href="~/Home/products?sub_cat_name=@drsubcat["subCatId"]">@drsubcat["subCatName"]</a>
                                                        </li>


                                                    }
                                                }
                                            </ul>
                                        </li>
                                    }
                                }


                            </ul>
                        </nav><!-- /.mainnav -->
                        @{
                            string mesage = Message;
                        }
                        <div class="product-content product-fivecolumn clearfix">
                            <ul class="product style3">
                                @{ con.Open();
                                    SqlCommand procmd = new SqlCommand("sp_Product", con);
                                    procmd.CommandType = CommandType.StoredProcedure;
                                    procmd.Parameters.AddWithValue("@Action", "S");
                                    SqlDataReader prosdr = procmd.ExecuteReader();
                                    DataTable dtpro = new DataTable();

                                    dtpro.Load(prosdr);
                                    con.Close();

                                    foreach (DataRow drpro in dtpro.Rows)
                                    {
                                        int proid = Convert.ToInt32(drpro["proId"]);
                                        int price = Convert.ToInt32(drpro["proDisPrice"]);

                                        <li class="product-item @drpro["catId"]">

                                            <div class="product-thumb clearfix">
                                                <a href="~/Home/Product_Details?proid=@proid">
                                                    <img src="~/Upload/Product/@drpro["proImage"]" style="height:195px;width:178px;" />
                                                </a>
                                                <span class="new ">Sale</span>
                                            </div>

                                            <div class="product-info clearfix">
                                                <span class="product-title">@drpro["proName"]<br> @drpro["subCatName"]</span>
                                                <div class="price">
                                                    <del>
                                                        <span class="regular">&#x20B9;@drpro["proPrice"]</span>
                                                    </del>
                                                    <ins>
                                                        <span class="amount">&#x20B9;@drpro["proDisPrice"]</span>
                                                    </ins>
                                                </div>
                                            </div>
                                            <div class="add-to-cart text-center">
                                                @{
                                                    DataTable _dt = new DataTable();
                                                    using (SqlCommand _sql = new SqlCommand("SELECT * FROM tblProAttr WHERE tblProAttr.proId=@proId ", con))
                                                    {

                                                        _sql.Parameters.AddWithValue("@proId", proid);
                                                        using (SqlDataAdapter adapterProImage = new SqlDataAdapter(_sql))
                                                        {
                                                            adapterProImage.Fill(_dt);
                                                        }
                                                    }
                                                    string size = "";
                                                    int color_id = 0;
                                                    int qty = 1;
                                                    if (_dt.Rows.Count > 0)
                                                    {
                                                        DataRow firstRow = _dt.Rows[0];
                                                        size = firstRow["size"].ToString();
                                                        color_id = Convert.ToInt32(firstRow["colorId"]);
                                                    }
                                                    <form method="post" action="~/Home/AddToCart">
                                                        @Html.AntiForgeryToken()


                                                        <input type="hidden" name="pid" value="@proid" id="pid" />
                                                        <input type="hidden" name="price" value="@price" id="price" />
                                                        <input type="hidden" name="qty" value="@qty" id="qty" />
                                                        <input type="hidden" name="color_id" value="@color_id" id="color_id" />
                                                        <input type="hidden" name="size_id" value="@size" id="size_id" />
                                                        <button type="submit">Add To Cart</button>
                                                    </form>
                                                }



                                            </div>

                                        </li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                </div><!-- /.row -->
            </div><!-- /.container -->
